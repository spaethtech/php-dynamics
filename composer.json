{
    "name": "spaethtech/php-dynamics",
    "description": "A library for dynamic objects and methods.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@spaethtech.com"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-json": "*",
        "spaethtech/common": "*",
        "spaethtech/annotations": "*",
        "spaethtech/collections": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "SpaethTech\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpaethTech\\": "./tests/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
